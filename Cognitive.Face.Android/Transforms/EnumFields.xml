<?xml version="1.0" encoding="UTF-8"?>
<enum-field-mappings>
	<!--
  This example converts the constants Fragment_id, Fragment_name,
  and Fragment_tag from android.support.v4.app.FragmentActivity.FragmentTag
  to an enum called Android.Support.V4.App.FragmentTagType with values
  Id, Name, and Tag.
  
  <mapping clr-enum-type="Android.Support.V4.App.FragmentTagType" jni-class="android/support/v4/app/FragmentActivity$FragmentTag">
    <field clr-name="Id" jni-name="Fragment_id" value="1" />
    <field clr-name="Name" jni-name="Fragment_name" value="0" />
    <field clr-name="Tag" jni-name="Fragment_tag" value="2" />
  </type>

  Notes:
  - An optional "bitfield" attribute marks the enum type with [Flags].
  - For Java interfaces, use "jni-interface" attribute instead of "jni-class" attribute.
  -->
	<mapping clr-enum-type="Xamarin.Cognitive.Face.Droid.Contract.TrainingStatusType" jni-class="com/microsoft/projectoxford/face/contract/TrainingStatus$Status">
		<field clr-name="Succeeded" jni-name="Succeeded" value="0" />
		<field clr-name="Failed" jni-name="Failed" value="1" />
		<field clr-name="Running" jni-name="Running" value="2" />
	</mapping>
	<mapping clr-enum-type="Xamarin.Cognitive.Face.Droid.Contract.GlassesType" jni-class="com/microsoft/projectoxford/face/contract/Glasses">
		<field clr-name="NoGlasses" jni-name="NoGlasses" value="0" />
		<field clr-name="ReadingGlasses" jni-name="ReadingGlasses" value="1" />
		<field clr-name="Sunglasses" jni-name="Sunglasses" value="2" />
		<field clr-name="SwimmingGoggles" jni-name="SwimmingGoggles" value="3" />
	</mapping>
</enum-field-mappings>